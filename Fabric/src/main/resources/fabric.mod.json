{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${mod_author}"
  ],
  "contact": {
    "sources": "https://github.com/lilypuree/Decorative-Blocks"
  },
  "license": "${mod_license}",
  "icon" : "logo.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "lilypuree.decorative_blocks.DecorativeBlocks"
    ],
    "client": [
      "lilypuree.decorative_blocks.ClientInitializer"
    ]
  },
  "mixins": [
    "${mod_id}.mixins.json",
    "${mod_id}.fabric.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14",
    "fabric": "*",
    "minecraft": ">=1.20.1",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
